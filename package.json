{
  "name": "parking-api",
  "version": "1.0.0",
  "description": "A real-world parking management system powered by a Node.js API",
  "main": "server.ts",
  "scripts": {
    "lint": "eslint .",
    "format": "eslint --fix .",
    "seed": "npx prisma db seed",
    "migrate": "dotenv -e .dev.env -- npx prisma db push",
    "predev": "rm -rf node_modules/.prisma && prisma generate",
    "dev": "cross-env NODE_ENV=dev nodemon --signal SIGHUP -r tsconfig-paths/register src/server.ts",
    "build": "tsc",
    "preprod": "rm -rf node_modules/.prisma && prisma generate && tsc",
    "prod": "cross-env NODE_ENV=prod ts-node -r tsconfig-paths/register dist/server.js"
  },
  "keywords": [],
  "author": "github.com/jeeva-sd",
  "license": "ISC",
  "engines": {
    "node": ">= 18.0.0"
  },
  "prisma": {
    "schema": "./src/database/schema.prisma",
    "seed": "dotenv -e .dev.env -- ts-node -r tsconfig-paths/register ./src/database/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.2",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.56.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2"
  }
}